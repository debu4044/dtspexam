 clc
 clear
 #Impulse Response
 M = 401;
 shift = (M-1) / 2;
 for n = 0 : 1 : M-1
  hds(n+1) = sin(0.317*pi*(n-shift))/(pi*(n-shift)) - sin(0.226*pi*(n-shift))/(pi*(n-shift));
 end
 hds(shift+1) = 0.09;
 figure(1)
 plot(hds)
 #Hanning Window
 for n = 0 : 1 : M-1
  wh(n+1) = 0.5 - 0.5*cos((2*pi*n) / (M-1));
 end
 #Multiply Impulse Response with window
 h = hds .* wh;
 figure(2)
 plot(h)
 #Checking the filter
 figure(3)
 freqz(h,1,M,44100)
 #Implementing
